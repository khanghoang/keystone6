  "dependencies": {
    "@keystone-6/core": "^1.0.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "cross-env": "^7.0.3",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "npm-run-all": "^4.1.5",
    "typescript": "4.5.4"
  }
}
